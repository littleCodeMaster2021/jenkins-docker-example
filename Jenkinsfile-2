pipeline {
    agent {
        docker { image 'jenkins/jenkins:lts' }
    }
    stages {
    
    stage('Initialize'){
        steps {
            def dockerHome = tool 'myDocker'
            env.PATH = "${dockerHome}/bin:${env.PATH}"
            }
    }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
    }
}
